{
  "font": {
    "button": {
      "default": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamily.primary}",
          "fontWeight": "{fontWeight.medium}",
          "fontSize": "{fontSize.100}",
          "lineHeight": "{lineHeight.small}"
        }
      }
    }
  },
  "sizing": {
    "button": {
      "height": {
        "default": {
          "$type": "sizing",
          "$value": "{dimension.x1000}"
        }
      }
    }
  },
  "borderRadius": {
    "button": {
      "default": {
        "$type": "borderRadius",
        "$value": "{borderRadius.small}"
      }
    }
  },
  "boxshadow": {
    "button": {
      "primary": {
        "$type": "boxShadow",
        "$value": {
          "x": "-4",
          "y": "4",
          "blur": "0",
          "spread": "0",
          "color": "{color.core.shadow.primary}",
          "type": "dropShadow"
        }
      }
    }
  }
}